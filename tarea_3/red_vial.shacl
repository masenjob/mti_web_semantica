:region_shape  a sh:NodeShape;
    sh:targetClass  :region ;
    sh:property [
        sh:path schema:name ;
        sh:minCount 1;
        sh:maxCount 1;
        sh:dataType xsd:string  ;
    ];
    sh:property [
        sh:path schema:num_region ;
        sh:minCount 1;
        sh:maxCount 1;
        sh:dataType xsd:string  ;
    ].


